{
	"env": {
		"browser": true,
		"node": true,
		"es2021": true
	},
	"extends": ["airbnb", "plugin:react"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true,
			"impliedStrict": true
		},
		"ecmaVersion": 12,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/prop-types": "off",
		"react/jsx-filename-extension": "off"
	}
}
